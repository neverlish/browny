<div class="container">
  <%= render "category" %>
  <br>
  <br>


	<h4><%= Place.find(@place_id).name if @place_id %> Spots</h4>
	<% @spots.each do |spot| %>
		<div class="well">
			<%= render partial: "spots/show", locals: {spot: spot} %>
		</div>
	<% end %>

	<%= link_to 'Spot 만들기', new_spot_path, class: 'btn btn-default' %>
</div>