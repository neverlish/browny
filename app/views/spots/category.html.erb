<div class="container">
	<%= render "category" %>
	<br>
	<br>
	<h4><%= Place.find(@place_id).name if @place_id %></h4>
	<div class="row">
		<% @category_spots.each do |spot| %>
			<div class="col-sm-4 col-md-3">
	        <%= link_to spot_path(spot) do %>
	          <div class="thumbnail thumbnail-image-box" style="background-image: url(<%= spot.picture.url %>);">
	      			<div class="caption">
	          		<h3><%= spot.name%></h3>
	    				</div>
	          </div>
	        <% end %>
	     </div>
		<% end %>
	</div>
</div>