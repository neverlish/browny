
<h4>Places</h4>

<p><%= link_to 'Place 만들기', new_place_path, class: 'btn btn-default' %></p>

<div class="row">
<% @places.each do |place| %>
  <div class="col-sm-4 col-md-3">
    <div class="thumbnail">
    	<%= link_to place_path(place) do %>
      		<img src="..." alt="...">
      			<div class="caption">
        		<h3><%= place.name%></h3>
				</div>
		<% end %>
    </div>
  </div>
<% end %>
</div>